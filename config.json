{
  "export_formats": {
    "android_format_1": {
      "description": "Android M/D/YY, H:MM AM/PM format",
      "pattern": "(\\d{1,2}/\\d{1,2}/\\d{2,4}),?\\s+(\\d{1,2}:\\d{2})\\s*(AM|PM)?\\s*-\\s*([^:]+?):\\s*(.*)",
      "date_format": "%m/%d/%y",
      "time_format": "%I:%M %p",
      "groups": ["date", "time", "ampm", "sender", "message"]
    },
    "android_format_2": {
      "description": "Android DD/MM/YY, HH:MM format",
      "pattern": "(\\d{1,2}/\\d{1,2}/\\d{2,4}),?\\s+(\\d{1,2}:\\d{2})\\s*-\\s*([^:]+?):\\s*(.*)",
      "date_format": "%d/%m/%y",
      "time_format": "%H:%M",
      "groups": ["date", "time", "sender", "message"]
    },
    "ios_format_1": {
      "description": "iOS [M/D/YY, H:MM:SS AM/PM] format",
      "pattern": "\\[(\\d{1,2}/\\d{1,2}/\\d{2,4}),?\\s+(\\d{1,2}:\\d{2}:\\d{2})\\s*(AM|PM)?\\]\\s*([^:]+?):\\s*(.*)",
      "date_format": "%m/%d/%y",
      "time_format": "%I:%M:%S %p",
      "groups": ["date", "time", "ampm", "sender", "message"]
    },
    "iso_format": {
      "description": "ISO YYYY-MM-DD format",
      "pattern": "(\\d{4}-\\d{1,2}-\\d{1,2}),?\\s+(\\d{1,2}:\\d{2})\\s*-\\s*([^:]+?):\\s*(.*)",
      "date_format": "%Y-%m-%d",
      "time_format": "%H:%M",
      "groups": ["date", "time", "sender", "message"]
    }
  },
  "patterns": {
    "strong_wishes": [
      "happy birthday",
      "hbd",
      "bday",
      "happy bday",
      "many happy returns",
      "many many happy returns",
      "born day",
      "bornday",
      "happy b day",
      "happy b-day",
      "happy b'day",
      "hpy bday",
      "happyy birthday",
      "birthday wishes",
      "wish you a very happy birthday",
      "wishing you many many happy returns"
    ],
    "weak_signals": [
      "🎂", "🎉", "🥳", "🎁", "🎈", "🍰", "🎊"
    ],
    "thanks_patterns": [
      "thanks",
      "thank you",
      "thx",
      "appreciate",
      "grateful",
      "thanks everyone",
      "thank you all",
      "thanks guys",
      "thanks for the wishes",
      "appreciate the wishes",
      "thank u",
      "thnx",
      "tysm",
      "thank you so much"
    ],
    "modifiers": {
      "belated": ["belated", "late", "sorry i missed", "missed your"],
      "advance": ["advance", "early", "tomorrow", "advance wishes"]
    },
    "negative_patterns": [
      "anniversary",
      "work anniversary",
      "wedding anniversary"
    ],
    "name_extraction": {
      "mention_patterns": [
        "@(\\d+)",
        "@([a-zA-Z][a-zA-Z0-9_]*)",
        "(?:happy\\s+birthday|hbd|bday)[\\s,]+([a-zA-Z][a-zA-Z\\s]*?)(?:[!\\.,]|$)",
        "(?:to|for)\\s+([a-zA-Z][a-zA-Z\\s]*?)(?:[!\\.,]|$)"
      ],
      "name_prefixes": ["MS - ", "Mr. ", "Mrs. ", "Dr. "],
      "phone_patterns": [
        "\\+\\d{1,3}\\s?\\d{5}\\s?\\d{5}",
        "\\+\\d{1,3}\\s?\\d{10}",
        "@\\d{10,15}"
      ]
    }
  },
  "confidence": {
    "min_threshold": 0.3,
    "base_score": 0.3,
    "multi_year_bonus": 0.2,
    "unique_wishers_bonus": 0.2,
    "thanks_bonus": 0.15,
    "explicit_mention_bonus": 0.1,
    "conflicting_dates_penalty": -0.2,
    "modifier_penalty": -0.1
  },
  "clustering": {
    "window_hours": 36,
    "min_wishers": 1,
    "min_wish_score": 0.1
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/hbd_app.log",
    "max_bytes": 10485760,
    "backup_count": 5
  }
}